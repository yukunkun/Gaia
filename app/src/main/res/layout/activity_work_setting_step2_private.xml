<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".module_down_up_load.upload_manage.WorkSettingStep2Activity">

    <!--选择分类-->
    <include layout="@layout/item_upload_choose_category" />

    <include layout="@layout/item_upload_set_tag"/>
    <!--下载设置-->
    <include layout="@layout/item_upload_download_set" />

    <include layout="@layout/item_upload_add_to_special" />

    <!--<include layout="@layout/item_upload_back_story" />-->

    <Button
        android:layout_marginTop="10dp"
        android:layout_gravity="center_horizontal"
        android:id="@+id/work_setting_commit"
        android:text="@string/save_change"
        android:background="@drawable/shape_btn_next_step"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="36dp"/>

</LinearLayout>
